<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="10dp"
        android:shrinkColumns="*"
        android:stretchColumns="*">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="text_ana"
            android:textSize="20dp"
            android:textStyle="bold" />

        <TableRow>

            <Button
                android:id="@+id/eg_example_analytics_signin"
                android:onClick="onButtonClick"
                android:text="signin" />

            <Button
                android:id="@+id/eg_example_analytics_signoff"
                android:onClick="onButtonClick"
                android:text="signoff" />
        </TableRow>

        <TableRow>

            <Button
                android:id="@+id/eg_example_analytics_event"
                android:onClick="onButtonClick"
                android:text="normal_event" />

            <Button
                android:id="@+id/eg_example_analytics_ekv"
                android:onClick="onButtonClick"
                android:text="kv_event" />
        </TableRow>

        <Button
            android:id="@+id/eg_example_analytics_duration"
            android:layout_width="fill_parent"
            android:onClick="onButtonClick"
            android:text="event_duration" />

        <Button
            android:id="@+id/eg_example_analytics_make_crash"
            android:onClick="onButtonClick"
            android:text="make_crash" />

        <Button
            android:id="@+id/eg_example_analytics_js_analytic"
            android:onClick="onButtonClick"
            android:text="js_in_webview" />

        <TableRow>

            <Button
                android:id="@+id/eg_example_analytics_fragment_stack"
                android:layout_width="fill_parent"
                android:onClick="onButtonClick"
                android:text="page_stack" />

            <Button
                android:id="@+id/eg_example_analytics_fragment_tabs"
                android:layout_width="fill_parent"
                android:onClick="onButtonClick"
                android:text="page_tabs" />
        </TableRow>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="text_dplus"
            android:textSize="20dp"
            android:textStyle="bold" />

        <TableRow>

            <Button
                android:id="@+id/eg_example_event_dplus_nomal"
                android:layout_width="fill_parent"
                android:onClick="onButtonClick"
                android:text="event_dplus_nomal" />

            <Button
                android:id="@+id/eg_example_event_dplus"
                android:layout_width="fill_parent"
                android:onClick="onButtonClick"
                android:text="event_dplus" />
        </TableRow>

        <TableRow>

            <Button
                android:id="@+id/eg_example_dplus_set_supper"
                android:onClick="onButtonClick"
                android:text="set_supper" />

            <Button
                android:id="@+id/eg_example_dplus_get_suppers"
                android:layout_width="fill_parent"
                android:onClick="onButtonClick"
                android:text="get_suppers" />
        </TableRow>


        <Button
            android:id="@+id/eg_example_clear_suppers"
            android:onClick="onButtonClick"
            android:text="clear_suppers" />

        <Button
            android:id="@+id/eg_example_first_launch_event"
            android:onClick="onButtonClick"
            android:text="set_first_launch_event" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/eg_example_autobinding_note"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="autobinding_note"
                android:textColor="@android:color/darker_gray"
                android:textSize="12dp" />

            <ImageView
                android:id="@+id/eg_example_autobinding_imageview"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center_horizontal"
                android:onClick="onButtonClick"
                android:src="@drawable/beauty" />

            <TextView
                android:id="@+id/eg_example_autobinding_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="autobinding_textview"
                android:textSize="14dp" />

            <EditText
                android:id="@+id/eg_example_autobinding_edit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="autobinding_edittext" />

            <Button
                android:id="@+id/eg_example_autobinding_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="autobinding_button" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:gravity="center">

                <ToggleButton
                    android:id="@+id/toggleButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="autobinding_togglebutton" />

                <CheckBox
                    android:id="@+id/checkBox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:text="autobinding_checkbox" />

                <RadioButton
                    android:id="@+id/radioButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:text="autobinding_radiobutton" />

            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp">

                <SeekBar
                    android:id="@+id/seekBar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

            </RelativeLayout>

        </LinearLayout>


        <WebView
            android:id="@+id/test_activity_webView"
            android:layout_width="match_parent"
            android:layout_height="200dp"></WebView>
    </TableLayout>

</ScrollView>